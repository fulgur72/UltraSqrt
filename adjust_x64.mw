<Group labelreference="L1" drawlabel="true">
<Input>
<Text-field style="Text" layout="Normal"><Font bold="true" family="Calibri" underline="true" size="18">sqrt algorithm - next partial result adjust calculations</Font></Text-field>
</Input>
</Group>
<Group labelreference="L2" drawlabel="true">
<Input>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">Digits := 50:
num2hexstr := proc(num::integer, len::integer)
  local hexstr;
  hexstr := convert(num, hex);
  while length(hexstr) &lt; len do
    hexstr := cat(&quot;0&quot;, hexstr);
  od;
  hexstr := convert(hexstr, string);
end:
adjust_prob := proc(sq::integer, cycl::integer)
  local size, ee, num, l, n, nn, p0, p1, p2, c0, c1, c2;
  size := 64;
  ee := trunc(log[4](sq));
  num := evalf(sqrt(sq)*2^(size-1-ee));
  l := trunc(num);
  n := trunc((num-l)*2^size);
  nn := 2^size-1-n;
  if nn &lt;= l then
    p0 := evalf(1-nn/2/l);
    p1 := 1;
  elif nn &lt;= 2*l then
    p0 := evalf(l/2/nn);
    p1 := evalf(2-l/2/nn-nn/2/l);
  else
    p0 := evalf(l/2/nn);
    p1 := evalf(3*l/2/nn);
  fi;
  p2 := 1 - p1;
  p1 := p1 - p0;
  c2 := round(p2*cycl);
  c1 := round((p1+p2)*cycl)-c2;
  c0 := cycl-c1-c2;
<Font encoding="UTF-8">  printf(&quot;sqrt(%d)\134n&quot;,sq);
  printf(&quot;binar_lead: 0x%s\134n&quot;, num2hexstr(l, size/4));
  printf(&quot;binar_next: 0x%s\134n&quot;,</Font> num2hexstr(n, size/4));
<Font encoding="UTF-8">  printf(&quot;Adjust 0x probability: %10.6f %% (~ %8d cycles)\134n&quot;, 100*p0, c0);
  printf(&quot;Adjust 1x probability: %10.6f %% (~ %8d cycles)\134n&quot;, 100*p1, c1);
  printf(&quot;Adjust 2x probability: %10.6f %% (~ %8d cycles)\134n&quot;, 100*p2, c2);
</Font>end:
</Text-field>
</Input>
</Group>
